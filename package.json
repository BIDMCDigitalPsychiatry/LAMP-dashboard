    "jspdf": "^3.0.1",
    "lamp-core": "2025.06.04",
    "material-icons": "^1.10.11",
    "monaco-editor": "^0.33.0",
    "notistack": "^2.0.5",
    "os": "^0.1.2",
    "qrcode.react": "^1.0.0",
    "react": "^17.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dnd": "^14.0.4",
