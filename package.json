{
  "name": "lamp_dashboard",
  "version": "1.0.0",
  "author": "BIDMC Division of Digital Psychiatry",
  "description": "LAMP Dashboard",
  "private": true,
  "homepage": ".",
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-is": "^16.7.1",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "2.0.4",
    "react-scripts": "^4.0.3",
    "typescript": "3.8",
    "fs-extra": "10.0.0",
    "child-process": "1.0.2"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@koale/useworker": "^3.4.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.48",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/icons": "4.11.2",
    "@rjsf/core": "^2.5.1",
    "@rjsf/material-ui": "^2.5.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.0.0-beta.5",
    "file-saver": "^1.3.8",
    "i18next": "^19.8.3",
    "i18next-http-backend": "^1.0.21",
    "idb": "^6.0.0",
    "javascript-time-ago": "^2.0.4",
    "lamp-core": "^1.0.8",
    "material-icons": "^0.5.4",
    "notistack": "^0.9.9",
    "qrcode.react": "^1.0.0",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.8",
    "react-error-boundary": "^3.1.0",
    "react-i18next": "^11.7.3",
    "react-images-upload": "^1.2.8",
    "react-jss": "^8.6.1",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.0.1",
    "react-vega": "^7.4.1",
    "regenerator-runtime": "^0.13.3",
    "remark-emoji": "^2.1.0",
    "remark-gfm": "^1.0.0",
    "stacktrace-js": "^2.0.2",
    "url-search-params-polyfill": "^7.0.0",
    "vega": "^5.17.1",
    "vega-lite": "^4.17.0",
    "vega-tooltip": "^0.24.2"
  },
  "scripts": {
    "start": "node buildEnv dev && react-scripts start",
    "start-sw": "http-server -p 3000 ./build",
    "test": "echo \"no tests configured\"",
    "build": "node buildEnv && react-scripts build",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "storybook": "start-storybook -p 3000 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  },
  "eslintConfig": {
    "rules": {
      "prettier/prettier": [
        "off",
        {
          "singleQuote": true
        }
      ]
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:prettier/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "react-hooks/exhaustive-deps": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
