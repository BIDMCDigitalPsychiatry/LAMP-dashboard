{
	"AWSEBDockerrunVersion": 1,
	"Image": {
		"privileged": "true"
	},
	"Ports": [
		{
			"ContainerPort": "9000"
		}
	],
	"Volumes": [
		{
			"HostDirectory": "/var/app/current",
			"ContainerDirectory": "/var/www/html"
		},
		{
			"HostDirectory": "/var/run/docker.sock",
			"ContainerDirectory": "/var/run/docker.sock"
		},
		{
			"HostDirectory": "/src",
			"ContainerDirectory": "/src"
		}
	]
}